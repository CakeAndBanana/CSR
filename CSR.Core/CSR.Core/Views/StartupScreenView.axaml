<UserControl xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="CSR.Core.Views.StartupScreenView">

  <DockPanel>
    <Button Click="StartButton_Click" Content="{Binding StartButton}" DockPanel.Dock="Top"></Button>
    <TextBlock FontSize="50" Text="{Binding Greeting}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
  </DockPanel>

</UserControl>
